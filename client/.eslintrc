{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "2018",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "eslint-config-prettier",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "no-console": "off"
  },
  "env": {
    "browser": true
  },
  "plugins": ["react", "jsx-a11y"]
}
